@{
    ViewBag.Title = "UnAssinClassRoom";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Unassign all Classrooms</title>
</head>
<body>
    <fieldset>
        <legend>Unassign all Classrooms</legend>
        <div align="center">
            <input type="submit" value="UnAssign All Class Room" id="UnallocateRooms" />
        </div>
    </fieldset>
</body>
</html>



<script src="~/Scripts/jquery-3.1.0.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script>
    $(document).ready(function () {
        $("#UnallocateRooms").click(function () {

            var r = confirm("Are you sure to unallocate all classrooms info?");
            if (r == true) {
                alert("All rooms unallocated successffully!");
            }
            
        });
    });

</script>
